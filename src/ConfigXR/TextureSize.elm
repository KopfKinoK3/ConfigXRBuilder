stOverride : TextureSize -> Maybe (List (Float, Float))
stOverride size =
    case size of
        Cm10x10 -> Nothing
        Cm sF tF ->
            let
                sMul = 10.0 / sF
                tMul = 10.0 / tF
            in
            Just <| List.map (\(s,t) -> ( s * sMul, t * tMul )) defaultSt

